Options +Includes
Options +FollowSymLinks

RewriteEngine On
RewriteCond %{REQUEST_URI} !^/COMP307/front-end/html/.*$
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /COMP307/front-end/html/$1 [L,R=301]
RewriteRule ^(/)?$ front-end/html/index.php [L]









